{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentUserName"
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/general.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14,
          "outputTemplate": "[Serilog {Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "SpeakStoreLocate.ApiSabervice"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "https://localhost:4200",
      "https://speakstorelocate.de",
      "https://www.speakstorelocate.de"
    ]
  },
  "AWS": {
    "AccessKey": "<set in env>",
    "SecretKey": "<set in env>",
    "Region": "eu-central-1"
  },
  "OpenAI": {
    "ApiKey": "<set in env>",
    "BaseUrl": "https://api.openai.com",
    "DefaultModel": "gpt-4.1-nano-2025-04-14",
    "Temperature": 0.0
  }
}
