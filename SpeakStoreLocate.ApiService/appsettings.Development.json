{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "https://localhost:4200",
      "http://localhost:5471",
      "https://localhost:5471",
      "http://localhost:3000",
      "https://localhost:3000",
      "http://127.0.0.1:4200",
      "http://127.0.0.1:5471",
      "http://127.0.0.1:3000"
    ]
  }
}
